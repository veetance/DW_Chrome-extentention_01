{
    "manifest_version": 2, 
    "name": "XYRooler",
    "description": "UPDATE: Use this tool to get accurate X,Y cordinates",
    "version": "1.0",
        




        "web_accessible_resources": [ 
        "images/People/*.jpg",
        "images/*.png"
       




     ],
     
   
    



    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "allFrames": true,
            "js": ["/js/jquery.js", "/js/content.js"],
            "css": ["/css/style.css"]
       }
     ],
    

   



 "background": {
 "scripts": ["/js/background.js"]
 


 },
     







   "browser_action": {
   "default_icon": "/js/MY_ICON.png"

    
  }

}
